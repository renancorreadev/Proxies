{
  "solidity.compileUsingRemoteVersion": "v0.8.20+commit.a1b79de6",
  "cSpell.words": ["ethersproject", "typechain"],
  "editor.fontSize": 12,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "eslint.alwaysShowStatus": true,
