		"body-parser": "^1.20.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"elliptic": "^6.5.4",
		"ethers": "^6.8.1",
		"ioredis": "^5.4.1",
		"minio": "^7.1.3",
		"node-vault": "^0.10.2",
		"passport-jwt": "^4.0.1",
		"passport-oauth2": "^1.8.0",
		"pg": "^8.11.1",
