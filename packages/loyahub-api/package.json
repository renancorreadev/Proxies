{
	"name": "loyahub-api",
	"version": "0.0.1",
	"description": "Projeto API do Client Manager Blockchain",
	"author": "Renan Correa",
	"private": true,
	"main": "dist/api/src/main.js",
	"scripts": {
		"te": "ts-node test/execute.ts",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"dev": "nest start --watch ",
		"debug": "nest start --debug --watch",
		"prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"lint:fix": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
		"test": "mocha --timeout 5000 --require ts-node/register -r tsconfig-paths/register './test/**/*.spec.ts'",
		"test:watch": "mocha --timeout 5000 --watch --require ts-node/register ./test/**/*.spec.ts",
		"test:cov": "nyc --reporter=lcov --timeout 5000 npm run test",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/mocha --runInBand",
		"test:single": "mocha --require ts-node/register -r tsconfig-paths/register",
		"clean": "rm -rf node_modules"
	},
	"dependencies": {
		"@nestjs/class-transformer": "^0.4.0",
